        "def dividee(sentence,position):\n",
        "  new = ''\n",
        "  old=''\n",
        "\n",
        "  for i in range(0,len(sentence)):\n",
        "    if (i !=0) and (i % position==0):\n",
        "      old += sentence[i]\n",
        "    else:\n",
        "      new += sentence[i]\n",
        "\n",
        "  return new +  old\n",
        "\n",
        "s= input() ; p= int(input())\n",
        "print(dividee(s,p))"
